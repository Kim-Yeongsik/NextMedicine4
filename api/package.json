    "babel-plugin-module-resolver": "^3.2.0",
    "babel-types": "^6.26.0",
    "csvtojson": "^2.0.10",
    "lorem-ipsum": "^2.0.3",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "prettier-tslint": "^0.4.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.3",
    "typescript-tslint-plugin": "^0.5.4"
  }
}
