{
  "name": "starter-babel-api",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "https://github.com/Tarasyn/starter/babel-api",
  "author": "Hojong Lee <hojonglee@limesociety.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npm-run-all build:types build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel . --extensions \".ts,.tsx\" --out-dir dist",
    "db:drop": "babel-node --extensions \".ts,.tsx\" scripts/db-drop.ts",
    "db:sync": "babel-node --extensions \".ts,.tsx\" scripts/db-sync.ts",
    "dev": "nodemon",
    "lint": "tslint --project .",
    "prettier": "prettier-tslint",
    "script": "babel-node --extensions \".ts,.tsx\"",
    "start": "cross-env NODE_ENV=production node dist",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "encoding": "^0.1.12",
    "express": "^4.17.1",
    "form-data": "^2.5.0",
    "helmet": "^3.20.0",
    "idx.macro": "^4.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.0",
    "nodemailer-direct-transport": "^3.3.2",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/cookie-session": "^2.0.37",
    "@types/cors": "^2.8.5",
    "@types/csvtojson": "^1.1.5",
    "@types/dotenv": "^6.1.1",
    "@types/form-data": "^2.5.0",
    "@types/helmet": "^0.0.43",
    "@types/lodash": "^4.14.136",
    "@types/morgan": "^1.7.36",
    "@types/multer": "^1.3.7",
    "@types/node": "^12.6.8",
    "@types/nodemailer": "^6.2.1",
    "@types/nodemailer-direct-transport": "^1.0.31",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
